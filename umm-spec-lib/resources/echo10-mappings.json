{
  "to-umm": {
    // TODO there's no need to have a definitions map if we only nest things and have no refs.
    // We may want to
    "root": "UMM-C",
    "definitions": {
      "UMM-C": {
        "type": "object",
        "properties": {
          // "MetadataLanguage": null,
          // "MetadataStandard": null,
          // "MetadataLineage": null,
          // "DirectoryName": null,
          "EntryTitle": {
            "type": "xpath",
            "value": "/Collection/DataSetId"
          },
          "Abstract": {
            "type": "xpath",
            "value": "/Collection/Description"
          },
          // "Purpose": null,
          // "DataLanguage": null,
          // "DataLineage": null,
          // "ResponsibleOrganization": null,
          // "ResponsiblePersonnel": null,
          // "CollectionDataType": null,
          // "ProcessingLevel": null,
          // "CollectionCitation": null,
          // "CollectionProgress": null,
          // "Quality": null,
          // "UseConstraints": null,
          // "AccessConstraints": null,
          // "Distribution": null,
          // "PublicationReference": null,
          // "ISOTopicCategory": null,
          // "ScienceKeyword": null,
          // "AncillaryKeyword": null,
          // "AdditionalAttribute": null,
          // "MetadataAssociation": null,
          "TemporalExtent": {
            // I don't think this element should be an array.
            "type": "array",
            // There's only one of these but this still works.
            "items-xpath": "/Collection/Temporal",
            "items": {
              "type": "object",
              "properties": {
                "TemporalRangeType": {
                  "type": "xpath",
                  "value": "TemporalRangeType"
                },
                "PrecisionOfSeconds": {
                  "type": "xpath",
                  "value": "PrecisionOfSeconds"
                },
                "EndsAtPresentFlag": {
                  "type": "xpath",
                  "value": "EndsAtPresentFlag"
                },
                "RangeDateTime": {
                  "type": "array",
                  "items-xpath": "RangeDateTime",
                  "items": {
                    "type": "matches-xml"
                  }
                },
                "SingleDateTime": {
                  "type": "array",
                  "items-xpath": "SingleDateTime"
                },
                "PeriodicDateTime": {
                  "type": "array",
                  "items-xpath": "PeriodicDateTime",
                  "items": {
                    "type": "matches-xml"
                  }
                }
              }
            }
          },
          // "PaleoTemporalCoverage": null,
          // "TemporalKeyword": null,
          // "SpatialExtent": null,
          // "TilingIdentificationSystem": null,
          // "SpatialInformation": null,
          // "SpatialKeyword": null,
          // "Platform": null,
          // "Project": null,
          // "RelatedUrl": null,
          "EntryId": {
            "type": "object",
            "properties": {
              "Id": {
                "type": "concat",
                "parts": [{
                  "type": "xpath",
                  "value": "/Collection/ShortName"
                }, {
                  "type": "constant",
                  "value": "_"
                }, {
                  "type": "xpath",
                  "value": "/Collection/VersionId"
                }]
              }
            }
          },
          "Product": {
            "type": "object",
            "properties": {
              "ShortName": {
                "type": "xpath",
                "value": "/Collection/ShortName"
              },
              "VersionId": {
                "type": "xpath",
                "value": "/Collection/VersionId"
              }
            }
          }
        }
      }
    }
  },
  "to-xml": {
    "root": "Collection",
    "definitions": {
      "Collection": {
        "type": "object",
        "properties": [
          ["ShortName", {
            "type": "mpath",
            "value": "/UMM-C/Product/ShortName"
          }],
          ["VersionId", {
            "type": "mpath",
            "value": "/UMM-C/Product/VersionId"
          }],
          ["DataSetId", {
            "type": "mpath",
            "value": "/UMM-C/EntryTitle"
          }],
          ["Description", {
            "type": "mpath",
            "value": "/UMM-C/Abstract"
          }],
          ["Temporal", {
            // TODO I'm not sure if array is appropriate here since there won't be multiple
            "type":"array",
            // ECHO10 only has one Temporal element so we can only select the first one.
            "items-mpath":"/UMM-C/TemporalExtent[1]",
            "items": {
              "type": "object",
              "properties": {
                "TemporalRangeType": {
                  "type": "mpath",
                  "value": "TemporalRangeType"
                },
                "PrecisionOfSeconds": {
                  "type": "mpath",
                  "value": "PrecisionOfSeconds"
                },
                "EndsAtPresentFlag": {
                  "type": "mpath",
                  "value": "EndsAtPresentFlag"
                },
                "RangeDateTime": {
                  "type": "array",
                  "items-mpath": "RangeDateTime",
                  "items": {
                    "type": "matches-umm"
                  }
                },
                "SingleDateTime": {
                  "type": "array",
                  "items-mpath": "SingleDateTime"
                },
                "PeriodicDateTime": {
                  "type": "array",
                  "items-mpath": "PeriodicDateTime",
                  "items": {
                    "type": "matches-umm"
                  }
                }
              }
            }
          }]
        ]
      }
    }
  }
}