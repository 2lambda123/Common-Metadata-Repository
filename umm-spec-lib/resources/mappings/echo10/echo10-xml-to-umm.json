{
  "UMM-C": {
    "type": "object",
    "properties": {
      // "MetadataLanguage": null,
      // "MetadataStandard": null,
      // "MetadataLineage": null,
      // "DirectoryName": null,
      "EntryTitle": {
        "type": "xpath",
        "value": "/Collection/DataSetId"
      },
      "Abstract": {
        "type": "xpath",
        "value": "/Collection/Description"
      },
      // "Purpose": null,
      // "DataLanguage": null,
      // "DataLineage": null,
      // "ResponsibleOrganization": null,
      // "ResponsiblePersonnel": null,
      // "CollectionDataType": null,
      // "ProcessingLevel": null,
      // "CollectionCitation": null,
      // "CollectionProgress": null,
      // "Quality": null,
      // "UseConstraints": null,
      // "AccessConstraints": null,
      // "Distribution": null,
      // "PublicationReference": null,
      // "ISOTopicCategory": null,
      // "ScienceKeyword": null,
      // "AncillaryKeyword": null,
      // "AdditionalAttribute": null,
      // "MetadataAssociation": null,
      "TemporalExtent": {
        // I don't think this element should be an array.
        "type": "array",
        // There's only one of these but this still works.
        "items-xpath": "/Collection/Temporal",
        "items": {
          "type": "object",
          "properties": {
            "TemporalRangeType": {
              "type": "xpath",
              "value": "TemporalRangeType"
            },
            "PrecisionOfSeconds": {
              "type": "xpath",
              "value": "PrecisionOfSeconds"
            },
            "EndsAtPresentFlag": {
              "type": "xpath",
              "value": "EndsAtPresentFlag"
            },
            "RangeDateTime": {
              "type": "array",
              "items-xpath": "RangeDateTime",
              "items": {
                "type": "object",
                "properties": {
                  "BeginningDateTime": {
                    "type": "xpath",
                    "value": "BeginningDateTime"
                  },
                  "EndingDateTime": {
                    "type": "xpath",
                    "value": "EndingDateTime"
                  }
                }
              }
            },
            "SingleDateTime": {
              "type": "array",
              "items-xpath": "SingleDateTime"
            },
            "PeriodicDateTime": {
              "type": "array",
              "items-xpath": "PeriodicDateTime",
              "items": {
                "type": "object",
                "properties": {
                  "Name": {
                    "type": "xpath",
                    "value": "Name"
                  },
                  "StartDate": {
                    "type": "xpath",
                    "value": "StartDate"
                  },
                  "EndDate": {
                    "type": "xpath",
                    "value": "EndDate"
                  },
                  "DurationUnit": {
                    "type": "xpath",
                    "value": "DurationUnit"
                  },
                  "DurationValue": {
                    "type": "xpath",
                    "value": "DurationValue"
                  },
                  "PeriodCycleDurationUnit": {
                    "type": "xpath",
                    "value": "PeriodCycleDurationUnit"
                  },
                  "PeriodCycleDurationValue": {
                    "type": "xpath",
                    "value": "PeriodCycleDurationValue"
                  }
                }
              }
            }
          }
        }
      },
      // "PaleoTemporalCoverage": null,
      // "TemporalKeyword": null,
      // "SpatialExtent": null,
      // "TilingIdentificationSystem": null,
      // "SpatialInformation": null,
      // "SpatialKeyword": null,
      // "Platform": null,
      // "Project": null,
      // "RelatedUrl": null,
      "EntryId": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "concat",
            "parts": [{
              "type": "xpath",
              "value": "/Collection/ShortName"
            }, {
              "type": "constant",
              "value": "_"
            }, {
              "type": "xpath",
              "value": "/Collection/VersionId"
            }]
          }
        }
      },
      "Product": {
        "type": "object",
        "properties": {
          "ShortName": {
            "type": "xpath",
            "value": "/Collection/ShortName"
          },
          "VersionId": {
            "type": "xpath",
            "value": "/Collection/VersionId"
          }
        }
      }
    }
  }
}