{:plugin {
   :jarfiles {
     ;; The reducer factory function must take two args: plugin name and plugin
     ;; type -- both of type ^String.
     :reducer-factory cmr.plugin.jar.core/create-regex-plugin-type-reducer}
   :registry {
     :component-key :plugin
     :default {
       :plugin-name "CMR-Plugin"
       :plugin-type ".*"
       :config-file "cmr-opendap/config.edn" ; the in-JAR path to the config file
       }
     :web {
       :route-keys [:web :route-fns]
       :api-route-key :api
       :site-route-key :site}}}
 :logging {
   :level :debug
   :nss [cmr]
   :color true}}
